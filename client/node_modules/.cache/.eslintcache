 await axios.post(\"/auth/logout\",inputs);\r\n        setCurrentUser(null);\r\n    }\r\n    useEffect(()=>{\r\n        localStorage.setItem(\"user\",JSON.stringify(currentUser));\r\n\r\n    },[currentUser]);\r\n    return (\r\n        <AuthContext.Provider value={{currentUser,login,logout}}>{children}</AuthContext.Provider>\r\n\r\n    )\r\n}",{"ruleId":"73","severity":1,"message":"74","line":4,"column":3,"nodeType":"75","messageId":"76","endLine":4,"endColumn":8},{"ruleId":"73","severity":1,"message":"77","line":25,"column":11,"nodeType":"75","messageId":"76","endLine":25,"endColumn":14},{"ruleId":"73","severity":1,"message":"78","line":15,"column":10,"nodeType":"75","messageId":"76","endLine":15,"endColumn":13},{"ruleId":"73","severity":1,"message":"79","line":15,"column":15,"nodeType":"75","messageId":"76","endLine":15,"endColumn":21},{"ruleId":"80","severity":1,"message":"81","line":6,"column":7,"nodeType":"82","endLine":6,"endColumn":24},{"ruleId":"73","severity":1,"message":"77","line":16,"column":15,"nodeType":"75","messageId":"76","endLine":16,"endColumn":18},"no-unused-vars","'Route' is defined but never used.","Identifier","unusedVar","'res' is assigned a value but never used.","'img' is assigned a value but never used.","'setImg' is assigned a value but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]